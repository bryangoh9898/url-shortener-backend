const mongoose = require('mongoose');
const Schema = mongoose.Schema;

const ShortUrlSchema = new Schema({
    fullUrl: {
        type: String,
        required: true
    },
    short:{
        type: String,
        required: true,
    }
},{
    timestamps: true
});



var ShortUrl = mongoose.model('ShortUrl' , ShortUrlSchema);

module.exports = ShortUrl;